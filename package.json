{
  "name": "boilerplate-react",
  "version": "0.0.1",
  "description": "Marc Dubois Boilerplate",
  "scripts": {
    "build": "TYPE_ENV=production webpack --mode production && cp public/manifest.json dist/manifest.json",
    "start": "npm run build",
    "client": "TYPE_ENV=development webpack serve --host 0.0.0.0 --mode development --devtool inline-source-map --hot",
    "dev": "concurrently \"npm run client\""
  },
  "author": "Marc Dubois",
  "license": "ISC",
  "dependencies": {
    "autosize": "^5.0.1",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "css-loader": "^5.2.6",
    "eslint": "^7.29.0",
    "eslint-plugin-react": "^7.24.0",
    "html-webpack-plugin": "^5.3.2",
    "nodemon": "^2.0.9",
    "style-loader": "^3.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
